{
    "name": "ndxbn/ndxbn_dip_jp",
    "description": "My home web services.",
    "keywords": [],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.2",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
